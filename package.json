    "@angular/platform-browser-dynamic": "~5.1.0",
    "@angular/router": "~5.1.0",
    "bootstrap": "~4.0.0-beta",
    "core-js": "~2.5.0",
    "normalize.css": "~7.0.0",
    "rxjs": "~5.5.0",
    "zone.js": "~0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "~1.6.0",

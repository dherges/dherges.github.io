  "scripts": {
    "ng": "ng",
    "build": "yarn run build:prod",
    "build:dev": "ng build --target=dev",
    "build:prod": "ng build --target=production --aot --build-optimizer --extract-css false",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "start": "yarn serve",
    "serve": "ng serve --extract-css",
    "serve:dist": "http-server dist",
    "test": "ng test",
    "predeploy": "yarn build:prod",
    "deploy": "gh-pages --dist dist --branch master --dotfiles"
  },
  "dependencies": {
    "@angular/common": "~5.1.0",
